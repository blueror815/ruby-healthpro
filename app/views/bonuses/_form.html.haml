= form_for @bonuse do |f|
  - if @bonuse.errors.any?
    #error_explanation
      %h2= "#{pluralize(@bonuse.errors.count, "error")} prohibited this bonuse from being saved:"
      %ul
        - @bonuse.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :price
    = f.text_field :price
  .field
    = f.label :timesheet_id
    = f.number_field :timesheet_id
  .actions
    = f.submit 'Save'
