%h1 Listing license_types

%table
  %thead
    %tr
      %th Name
      %th Code
      %th Job type
      %th
      %th
      %th

  %tbody
    - @license_types.each do |license_type|
      %tr
        %td= license_type.name
        %td= license_type.code
        %td= license_type.job_type_id
        %td= link_to 'Show', license_type
        %td= link_to 'Edit', edit_license_type_path(license_type)
        %td= link_to 'Destroy', license_type, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New License type', new_license_type_path
